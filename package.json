{
  "name": "react-engine",
  "version": "5.0.0",
  "description": "a composite render engine for express apps to render both plain react views and react-router views",
  "main": "index.js",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "test": "npm run lint && jest",
    "lint": "eslint lib __tests__"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paypal/react-engine"
  },
  "bugs": {
    "url": "https://github.com/paypal/react-engine/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "debug": "^2.1.3",
    "glob": "^7.0.5",
    "jsesc": "^2.2.0",
    "lodash": "^4.13.1",
    "parent-require": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.3.26",
    "babel-eslint": "^8.0.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "express": "^4.12",
    "jest": "^21.2.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^4.2.0"
  },
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.{js}"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 6
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "jest": true
    }
  },
   "eslintIgnore": [
     "examples/movie catalog/node_modules",
     "examples/movie catalog/public/bundle.js"
   ],
  "keywords": [
    "react",
    "render",
    "render engine",
    "react-router",
    "view engine",
    "express",
    "jsx"
  ],
  "author": "Sam Selvanathan <sjasel@gmail.com>",
  "contributors": [
    "Vu Hwang <vuhwang@gmail.com>",
    "Robert Kuo <slkuo230@yahoo.com>",
    "Jinto Jose <jintoppy@gmail.com>",
    "Jared Halpert <jaredhalpert90@gmail.com>",
    "Weng Zhi Ping",
    "Vincent Orr <vincent@cmdv.org>",
    "skarflacka",
    "Mark <mark@remarkablemark.org>"
  ],
  "license": "Apache-2.0"
}
